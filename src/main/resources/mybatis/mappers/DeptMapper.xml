<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.gec.mapper.DeptMapper">
	<select id="getDeptById" resultType="com.gec.domain.Dept">
        select * from dept
        where id=#{id}
    </select>
    <select id="getList" resultType="com.gec.domain.Dept">
        select * from dept
    </select>

</mapper>